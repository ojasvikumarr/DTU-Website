{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/login.js",
    "pages/_document.js",
    "pages/index.js",
    ".next/types/**/*.ts"
, "components/ui/aurora-background.jsx", "components/ui/badge.jsx", "components/ui/input.jsx", "components/ui/typewriter-effect.jsx", "components/ui/tooltip.jsx", "components/ui/select.jsx"  ],
  "exclude": [
    "node_modules"
  ],
  "baseUrl": ".",
  "paths": {
    "@/*": [
      "*"
    ]
  }
}
